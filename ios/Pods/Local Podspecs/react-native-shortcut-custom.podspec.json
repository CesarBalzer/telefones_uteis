{
  "name": "react-native-shortcut-custom",
  "version": "0.4.11",
  "summary": "Create shortcuts, work contacts and other ideas",
  "homepage": "https://github.com/CesarBalzer/react-native-shortcut-custom#readme",
  "license": "MIT",
  "authors": {
    "name": "Cesar Balzer",
    "url": "https://github.com/CesarBalzer"
  },
  "platforms": {
    "ios": "12.4"
  },
  "source": {
    "git": "https://github.com/CesarBalzer/react-native-shortcut-custom.git",
    "tag": "0.4.11"
  },
  "source_files": "ios/**/*.{h,m,mm,swift}",
  "compiler_flags": "-DFOLLY_NO_CONFIG -DFOLLY_MOBILE=1 -DFOLLY_USE_LIBCPP=1 -Wno-comma -Wno-shorten-64-to-32",
  "dependencies": {
    "React-Core": [

    ],
    "RCT-Folly": [
      "2021.07.22.00"
    ]
  },
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "\"$(PODS_ROOT)/boost\"",
    "CLANG_CXX_LANGUAGE_STANDARD": "c++17"
  }
}
